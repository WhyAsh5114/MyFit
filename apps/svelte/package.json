{
	"name": "myfit",
	"private": true,
	"version": "4.0.0",
	"type": "module",
	"prisma": {
		"schema": "./prisma/schema"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsx src/lib/scripts/precache.ts && tsx src/lib/scripts/routes.ts --create-json-file",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "playwright test",
		"postinstall": "prisma generate --no-engine"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.28.0",
		"@internationalized/date": "^3.8.1",
		"@playwright/test": "^1.52.0",
		"@prisma-idb/idb-client-generator": "^0.17.0",
		"@prisma/client": "^6.8.2",
		"@serwist/build": "^9.0.14",
		"@serwist/vite": "^9.0.14",
		"@serwist/window": "^9.0.14",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/enhanced-img": "^0.6.0",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.1.8",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^22.15.29",
		"@types/uuid": "^10.0.0",
		"bits-ui": "^2.4.0",
		"bridge-types": "workspace:*",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.0",
		"formsnap": "^2.0.1",
		"globals": "^16.2.0",
		"lucide-svelte": "^0.511.0",
		"mode-watcher": "^1.0.7",
		"paneforge": "1.0.0-next.5",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"prisma": "^6.8.2",
		"serwist": "^9.0.14",
		"svelte": "^5.33.10",
		"svelte-check": "^4.2.1",
		"svelte-sonner": "^1.0.4",
		"sveltekit-superforms": "^2.25.0",
		"tailwind-merge": "^3.3.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.8",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.0",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^6.3.5",
		"zod": "^3.25.42"
	},
	"dependencies": {
		"@tanstack/svelte-query": "^5.79.0",
		"@tanstack/table-core": "^8.21.3",
		"better-auth": "^1.2.8",
		"idb": "^8.0.3",
		"runed": "^0.28.0",
		"uuid": "^11.1.0"
	}
