{
	"name": "myfit",
	"private": true,
	"version": "4.0.0",
	"type": "module",
	"prisma": {
		"schema": "./prisma/schema"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsx src/lib/scripts/precache.ts && tsx src/lib/scripts/routes.ts --create-json-file",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "playwright test",
		"postinstall": "prisma generate --no-engine"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.8",
		"@eslint/js": "^9.25.0",
		"@internationalized/date": "^3.8.0",
		"@playwright/test": "^1.52.0",
		"@prisma-idb/idb-client-generator": "^0.17.0",
		"@prisma/client": "^6.6.0",
		"@prisma/extension-accelerate": "^1.3.0",
		"@serwist/build": "^9.0.14",
		"@serwist/vite": "^9.0.14",
		"@serwist/window": "^9.0.14",
		"@sveltejs/adapter-vercel": "^5.7.0",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.1.4",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^22.14.1",
		"@types/uuid": "^10.0.0",
		"bits-ui": "^1.3.19",
		"bridge-types": "workspace:*",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"eslint": "^9.25.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"formsnap": "^2.0.1",
		"globals": "^16.0.0",
		"lucide-svelte": "0.501.0",
		"mode-watcher": "^1.0.2",
		"paneforge": "1.0.0-next.5",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.6.0",
		"serwist": "^9.0.14",
		"svelte": "^5.28.1",
		"svelte-check": "^4.1.6",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.24.1",
		"tailwind-merge": "^3.2.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.4",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.30.1",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^6.3.2",
		"zod": "^3.24.3"
	},
	"dependencies": {
		"@tanstack/svelte-query": "^5.74.4",
		"@tanstack/table-core": "^8.21.3",
		"better-auth": "^1.2.7",
		"idb": "^8.0.2",
		"uuid": "^11.1.0"
	}
