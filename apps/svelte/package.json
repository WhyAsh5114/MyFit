{
	"name": "myfit",
	"private": true,
	"version": "4.0.0",
	"type": "module",
	"prisma": {
		"schema": "./prisma/schema"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsx src/lib/scripts/precache.ts && tsx src/lib/scripts/routes.ts --create-json-file",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e",
		"postinstall": "prisma generate --no-engine"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.7.0",
		"@playwright/test": "^1.49.1",
		"@prisma-idb/idb-client-generator": "^0.x.0",
		"@prisma/client": "^6.5.0",
		"@prisma/extension-accelerate": "^1.2.2",
		"@serwist/build": "^9.0.12",
		"@serwist/vite": "^9.0.12",
		"@serwist/window": "^9.0.12",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/postcss": "^4.0.13",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^22.13.10",
		"bits-ui": "^1.3.11",
		"bridge-types": "^1.0.0",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.5.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.1.0",
		"formsnap": "^2.0.0",
		"globals": "^16.0.0",
		"lucide-svelte": "^0.x.x",
		"mdsvex": "^0.12.3",
		"mode-watcher": "^0.5.1",
		"paneforge": "^1.0.0-next.4",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.5.0",
		"serwist": "^9.0.12",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.24.0",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.13",
		"tailwindcss-animate": "^1.0.7",
		"tsx": "^4.19.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vaul-svelte": "^1.0.0-next.6",
		"vite": "^6.0.0",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"@tanstack/svelte-query": "^5.68.0",
		"@tanstack/table-core": "^8.21.2",
		"better-auth": "^1.2.4"
	}
}
